{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "models",
    "module": "commonjs",
    "compilerOptions": {
        "target": "es6",
        "moduleResolution": "node",
        "composite": false,
        "declaration": true,
        "declarationMap": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "inlineSources": false,
        "isolatedModules": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "preserveWatchOutput": true,
        "skipLibCheck": true,
        "strict": true,
        "lib": [
            "ES6"
        ],
        "module": "ES6",
        "rootDir": "./src",
        "outDir": "./dist"
    },
    "include": [
        "src"
    ],
    "exclude": [
        "node_modules"
    ]
}